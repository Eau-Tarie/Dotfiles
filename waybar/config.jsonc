{
    "layer": "bottom",
    "position": "top",
    "height": 40,
    "margin-top": 5,
    "margin-left": 10,
    "margin-right": 10,
    "margin-bottom": 10,
    "spacing": 10,
    "modules-left": [
	"group/desktop",
	//"group/power",
	"hyprland/workspaces",
        "custom/media"
    ],
    "modules-center": [
       "group/hyprland"
    ],
    "modules-right": [
        "mpd",
	//"cava",
	"network",
	"pulseaudio",
	"bluetooth",
	"group/hardware",
	"custom/notification",
	"group/time"
    ],
    "group/hyprland": {
	"orientation": "horizontal",
	"modules": [
		"group/hyprlandWindow"
	]
    },
    "group/hyprlandWindow": {
	"orientation": "horizontal",
	"modules": [
		"hyprland/window",
		"hyprland/windowcount"
	]
    },
    "group/desktop":{
	"orientation": "horizontal",
	"modules": [
		"custom/logo",
		"group/power"
		]
    },
    "group/lock": {
	"orientation": "horizontal",
	"modules": [
		"custom/lock",
		"custom/logout"
	],
	"drawer": {
		"transition-duration": 500,
		"transition-left-to-right": true
		}
    },
    "group/suspend": {
	"orientation": "horizontal",
	"modules": [
		"custom/suspend",
		"custom/hibernate"
	],
	"drawer": {
		"transition-duration": 500,
		"transition-left-to-right": true
		}
    },
    "group/shutdown": {
	"orientation": "horizontal",
	"modules": [
		"custom/shutdown",
		"custom/reboot"
	],
	"drawer": {
		"transition-duration": 500,
		"transition-left-to-right": true
		}
    },
    "group/system": {
	"orientation": "horizontal",
	"modules": [
		"temperature",
		"cpu",
		"memory"
		//"disk"
		]
    },
    "group/power": {
	"orientation": "horizontal",
	"modules": [
		"group/lock",
		"group/suspend",
		"group/shutdown"
	]
    },
    "group/hardware": {
	    "orientation": "horizontal", 
	    "modules": [
		    "group/system",
		    "battery"
	    	]
    },
    "group/time": {
	"orientation": "horizontal",
	"modules": [
		"custom/clock",
		"clock"
		],
	"drawer": {
		"transition-duration": 500,
		"transition-left-to-right": false
	}
    },
    "custom/logo": {
	"format": "Û∞£á",
	"on-click": "rofi -show drun"
    }, 
    "hyprland/workspaces": {
	 "format": "{icon}",
	 "format-icons": {
		 "1": "<span color='#FFBB67'>1</span>",
		 "2": "<span color='#FFAA33'>2</span>",
		 "3": "<span color='#FF9900'>3</span>",
		 "4": "<span color='#FF8000'>4</span>",
		 "5": "<span color='#FF5E00'>5</span>",
		 "6": "<span color='#FF3C00'>6</span>"
	 },
	 "persistent-only": true,
	 "sort-by-id": true,
	 "persistent-workspaces": {
		 "*": [1, 2, 3, 4, 5, 6]
	 }
     },
    "hyprland/window": {
	    "max-length": 40,
	    "format": "{}",
    	    "rewrite": {
        	"(.*) ‚Äî Zen Browser": "<span color='#F0810F'><b>Û∞àπ  $1</b></span>",
        	"(.*)kitty": "Óûï  [$1]",
		"(.*) - code": "Ó£ö  $1",
		"(.*) IntelliJ IDEA": "<span color='#FF66CC'><b>Óûµ  $1</b></span>",
		"GitHub (.*)": "Ó™Ñ  $1",
		"nvim (.*)": "<span color='#71BD9A'><b>ÓöÆ  $1</b></span>",
		"(.*) - Thunar": "<span color='#E9B459'><b>Ôìì  $1</b></span>"
    	    },
    	    "separate-outputs": true
     },
    "hyprland/windowcount" : {
    	"format": " ",
    	"format-empty": " Û∞£á ",
    	"format-windowed": "Û∞äî",
    	"format-fullscreen": "Û∞äì",
    	"separate-outputs": true
     },
    "mpd": {
        "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{artist} - {album} - {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) ‚∏®{songPosition}|{queueLength}‚∏© {volume}% ÔÄÅ",
        "format-disconnected": "Disconnected ÔÄÅ",
        "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ÔÄÅ",
        "unknown-tag": "N/A",
        "interval": 5,
        "consume-icons": {
            "on": "ÔÉÑ "
        },
        "random-icons": {
            "off": "<span color=\"#f53c3c\">ÔÅ¥</span> ",
            "on": "ÔÅ¥ "
        },
        "repeat-icons": {
            "on": "ÔÄû "
        },
        "single-icons": {
            "on": "ÔÄû1 "
        },
        "state-icons": {
            "paused": "ÔÅå",
            "playing": "ÔÅã"
        },
        "tooltip-format": "MPD (connected)",
        "tooltip-format-disconnected": "MPD (disconnected)"
    },
    "cava": {
        "cava_config": "$XDG_CONFIG_HOME/cava/cava.conf",
        "framerate": 30,
        "autosens": 1,
        "sensitivity": 100,
        "bars": 14,
        "lower_cutoff_freq": 50,
        "higher_cutoff_freq": 10000,
        "hide_on_silence": false,
        // "format_silent": "quiet",
        "method": "pulse",
        "source": "auto",
        "stereo": true,
        "reverse": false,
        "bar_delimiter": 0,
        "monstercat": false,
        "waves": false,
        "noise_reduction": 0.77,
        "input_delay": 2,
        "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà" ],
        "actions": {
                   "on-click-right": "mode"
                   }
    },
    "tray": {
        "icon-size": 21,
	"show-passive-items": true,
        "spacing": 10,
        "icons": {
           "blueman": "bluetooth",
           "TelegramDesktop": "ÔãÜ"
        }
    },
    "clock": {
	"format": "{:%a %d %B %Y}",
        "tooltip-format": "<tt>{calendar}</tt>",
	"calendar": {
		"mode"		: "year",
		"mode-mon-col"	: 3,
		"weeks-pos"	: "right",
		"on-scroll"	: 1,
		"format"	: {
			  	  "months":	"<span color='#ffead3'><b>{}</b></span>",
			  	  "days":	"<span color='#ecc6d9'><b>{}</b></span>",
			  	  "weeks":	"<span color='#99ffdd'><b>W{}</b></span>",
			  	  "weekdays":	"<span color='#ffcc66'><b>{}</b></span>",
			  	  "today":	"<span color='#ff6699'><b><u>{}</u></b></span>"
		          	  }
		},
	"actions": {
		   "on-click-middle": "mode",
		   "on-click-right": "shift_reset",
		   "on-scroll-up": "shift_up",
		   "on-scroll-down": "shift_down"
	}
    },
    "custom/notification": {
  	"tooltip": false,
  	"format": "{} {icon} ",
  	"format-icons": {
    		"notification": "Û±Ö´",
    		"none": "ÔÉ≥",
    		"dnd-notification": "Ôá∂",
    		"dnd-none": "Û∞Çõ",
    		"inhibited-notification": "ÔÇ¢",
    		"inhibited-none": "ÔÇ¢",
    		"dnd-inhibited-notification": "Ôá∑",
    		"dnd-inhibited-none": "Ôá∑"
  	},
  	"return-type": "json",
  	"exec-if": "which swaync-client",
  	"exec": "swaync-client -swb",
  	"on-click": "sleep 0.1 && swaync-client -t -sw",
  	"on-click-right": "sleep 0.1 && swaync-client -d -sw",
  	"escape": true
    },    
    "custom/clock": {
	"exec": "~/.config/waybar/scripts/clock.sh",
	"interval": 1,
	"format": "{}"
    },
    "cpu": {
	"interval" : 5,
        "format": "{usage}% Ôãõ",
	"tooltip": true
    },
    "memory": {
	"interval": 5,
        "format": "{}% ÓøÖ ",
	"states" : {
		"good": 10,
		"low": 30,
		"average": 50,
		"warning": 75,
		"critical": 90
	},
	"tooltip-format": "{used:0.1f}G/{total:0.1f}G ÓøÖ"
    },
    "temperature": {
        "critical-threshold": 80,
	"format": "Ôãú {temperatureC}¬∞C",
        "format-critical": "Ôü§ {temperatureC}¬∞C"
    },
    "disk": {

	"format-charging-above-average": "{capacity}% Û∞Çâ",
	"format-charging-average": "{capacity}% Û∞¢ù",
	"format-charging-below-average": "{capacity}% Û∞Çà",
	"format-charging-low": "{capacity}% Û∞Çá",
	"format-charging-warning": "{capacity}% Û∞Çá",
	"format-charging-critical": "{capacity}% Û∞ÇÜ",
	"format-charging-almost-empty": "{capacity}% Û∞ÇÜ",
	"format-charging-empty": "{capacity}% Û∞¢ú",
        "format-alt": "{time}",
        // "format-good": "", // An empty format will hide the module
        // "format-full": "",
        "format-icons": {
		"full": "Û∞Åπ",
		"almost-full": "Û∞ÇÇ",
		"very-good": "Û∞ÇÅ",
		"good": "Û∞ÇÄ",
		"above-average": "Û∞Åø",
		"average": "Û∞Åæ",
		"below-average": "Û∞ÅΩ",
		"low": "Û∞Åº",
		"warning": "Û∞Åº",
		"critical": "Û∞Åª",
		"almost-empty": "Û∞Åª",
		"empty": "Û∞Å∫"
	}
    },
    "power-profiles-daemon": {
      "format": "{icon}",
